#Get one mark from student

mark=int(input("enter your score   "))
if (0<mark<100):
    print(mark,"is valid")
    if (50<mark<100):
        print("pass")
    if 90<mark<100:
        print("A")
    elif 80<mark<89:
        print("B")
    elif 70<mark<79:
        print("C")
    elif 60<mark<69:
        print("D")
    elif 50<mark<59:
        print("E")
else:
    print("fail")

